void bio()
{
	char n1[20],e1[20],p1[10],a1[50],q1[10];
	printf("enter your name:\n");
	fflush(stdin);
	gets(n1);
	fflush(stdin);
	printf("enter your email:\n");
	gets(e1);
	fflush(stdin);
	printf("enter your phone no:\n");
	gets(p1);
	fflush(stdin);
	printf("enter your address:\n");
    gets(a1);
	fflush(stdin);
	printf("enter your qualification:\n");
	gets(q1);
	fflush(stdin);
	
	printf("you are successful enrolled in the scheme");	
}
